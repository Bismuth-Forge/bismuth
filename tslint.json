{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": false,
        "max-classes-per-file": false,
        "no-conditional-assignment": false,
        "one-variable-per-declaration": false,
        "prefer-for-of": false,
        "variable-name": [true, "allow-leading-underscore"]
    },
    "rulesDirectory": []
}